<html>
  <head>
    <meta charset="utf-8">
    <link href="Content/css/styles.css" rel="stylesheet" type="text/css">
  </head>
  <body>
    <h1>Your Address Book:</h1>
    <br>
    <h3>Contacts:</h3>
    <p>Here are all your contacts:</p>

  @foreach (var contact in Model)
        {
          <li><a href='/allcontacts/@contact.GetId()'>@contact.GetName()</a></li>
        }

    <p><a href="/">Return to the Home Page</a></p>
    <form action="/clear" method="post">
      <button type="submit">Clear All Contacts</button>
    </form>
  </body>
</html>
